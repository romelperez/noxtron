{
  "name": "examples-esbuild",
  "private": true,
  "engines": {
    "node": "^16.14",
    "npm": "^8.3"
  },
  "dependencies": {
    "solid-js": "^1.4.3"
  },
  "scripts": {
    "clean": "rm -rf ./build",
    "prebuild": "sh ./prebuild.sh",
    "build": "npm run prebuild && cross-env NODE_ENV=production node ./build.js",
    "dev": "npm run prebuild && node ./build.js",
    "start": "serve ./build -p 4000"
  }
}
